{
    "name": "nova-custom/dynamic-action-modal",
    "description": "Dynamic Action Modal for Laravel Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "NovaDynamicModal\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaDynamicModal\\DynamicFieldsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
